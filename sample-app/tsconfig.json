{
    "compilerOptions": {
      "jsx": "react",
      "module": "esnext",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "strictNullChecks": true,
      "target": "es6",
      "lib": ["es2015", "es2017", "dom"]
    },
    "include": [
      "src/**/*"
    ],
    "awesomeTypescriptLoaderOptions": {
      "reportFiles": [
        "src/**/*"
      ],
    //   "useBabel": true,
    //   "useCache": true
    }
}